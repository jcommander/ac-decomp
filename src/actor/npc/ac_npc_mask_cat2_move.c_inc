    actorx->world.position.z = 280.0f;
    actorx->shape_info.rotation.y = 0;
    if (mask_cat2_actor->npc_class.draw.main_animation_state == TRUE) {
        npc_info_mask = mask_cat2_actor->npc_class.npc_info.mask;
