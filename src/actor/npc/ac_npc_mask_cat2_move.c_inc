    int action_idx;
    if (mMsg_Check_not_series_main_wait(msg_p) == TRUE) {
        msg_num = 0x321a;
        action_idx = aNM2_MOVE_SDON_AND_PB_WAIT;
        if (aNM2_chk_mask_texture() == TRUE) {
            msg_num = RANDOM(3) + 0x31e1;
            action_idx = aNM2_MOVE_TALK_END_WAIT;
